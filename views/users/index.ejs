


<h1>All Users</h1>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Nome</th>
            <th>Email</th>
            <th>Gender</th>
            <th>DataNasc</th>
            <th>Senha</th>
        </tr>
    </thead>
    <tbody>
        <% users.forEach(user => { %>
            <tr>
                <td><%= user.id %></td>
                <td><%= user.nome %></td>
                <td><%= user.email %></td>
                <td><%= user.gender %></td>
                <td><%= user.dataNasc %></td>
                <td><%= user.senha %></td>
                <td>
                    <a href="/users/<%= user.id %>/edit">Visualizar</a>
                    <a href="/users/<%= user.id %>/edit">Editar</a>
                    <form action="/users/<%= user.id %>?_method=DELETE" method="POST">
                        <button type="submit" id="delete">Deletar</button>
                    </form>
                </td>
            </tr>
        <% }) %>
    </tbody>
</table>


